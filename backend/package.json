{
  "name": "ecom-visits-tracker--backend",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "dev2": "ts-node-dev src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "npx eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "npx eslint \"{src,test}/**/*.ts\" --fix",
    "format": "npx prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "swagger:generate": "ts-node src/config/generate-spec.ts",
    "swagger:view": "node src/config/view-spec.js"
  },
  "dependencies": {
    "apicache": "^1.6.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.1",
    "node-redis": "^0.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/apicache": "^1.6.7",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.30",
    "@types/redis": "^4.0.10",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.28.0",
    "globals": "^16.2.0",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  }
}